{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#1E91EA",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\uf07b",
            "home_icon": "üè°",
            "mapped_locations": [
              [
                "/Users/nicdeiri/code",
                "üöÄ"
              ],
              [
                "/home/nic/code",
                "üöÄ"
              ]
            ],
            "style": "agnoster"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#F08864",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue70c{{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "type": "dotnet"
        },
        {
          "background": "#2E465D",
          "foreground": "#f0f0f0",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb3b{{ end }}",
            "{{ if gt .Ahead 0 }}#8A4FFF{{ end }}",
            "{{ if gt .Behind 0 }}#2EC4B6{{ end }}"
          ],
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_ahead_icon": "\uf55d",
            "branch_behind_icon": "\uf545",
            "branch_gone_icon": "\uf9e7",
            "branch_icon": "\uf418 ",
            "branch_identical_icon": "\uf9e5",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \uf448<#E84855>{{ .Working.String }}</>{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} \uf6d8{{ end }}{{ if .Staging.Changed }} \uf046 <#2FDA4E>{{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0}}\uf0c7 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#100e23",
          "background": "#906cff",
          "template": " \uE235 {{ .Full }} "
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "template": " {{if .Major }} \uE718 {{ .Major }} {{end}}",
          "properties": {
            "display_mode": "always"
          }
        },
        {
          "type": "aws",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#FFA400",
          "properties": {
            "display_default": false
          },
          "template": " \uf270 {{.Profile}}"
        }
      ],
      "type": "prompt"
    },
    {
      "type": "newline"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#FFEE58",
          "leading_diamond": "\uf438",
          "style": "plain",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "#26C6DA",
          "properties": {
            "wsl": "",
            "wsl_separator": ""
          },
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "#66D372",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#e91e63{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "{{ if gt .Code 0 }} \u274c{{ else }} \uf42e{{ end }} ",
          "type": "exit"
        },
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "\ue0b1 ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}